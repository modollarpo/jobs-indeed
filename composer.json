    "keywords": [
        "jobs",
        "api client",
        "object",
        "indeed"
    ],
    "homepage": "https://github.com/modollarpo/jobs-indeed",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Karl Hughes",
            "email": "khughes.me@gmail.com",
            "homepage": "http://www.karllhughes.com"
        },
        {
            "name": "Steven Maguire",
            "email": "stevenmaguire@gmail.com",
            "homepage": "https://github.com/stevenmaguire"
        }
    ],
    "require": {
        "php": ">=7.4",
        "modollarpo/jobs-common": "dev-master"
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpunit/php-code-coverage": "^9.0",
        "mockery/mockery": "^1.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
